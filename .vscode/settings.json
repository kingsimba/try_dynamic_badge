{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        ".mypy_cache": true
    },
    "python.envFile": "${workspaceRoot}/.env",
    "python.pythonPath": "/usr/bin/python3",
    "python.linting.flake8Enabled": true,
    "python.linting.pylintEnabled": false,
    "python.linting.mypyEnabled": true
}